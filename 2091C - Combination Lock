#include<iostream>
using namespace std;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    int te; cin>>te;
    while( te--){
        int n ; cin>>n;
        if( n%2 == 0){
            cout<<-1<<"\n";
            continue;
        }
        int r = n;
        //int l = 2;
        cout<<1<<" ";
        for( int i = 2; i<=n;i++){
            cout<<r<<" ";
            r--;
        }
        cout<<"\n";
    }
}
