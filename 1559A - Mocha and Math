#include<iostream>
using namespace std;
#include<bits/stdc++.h>
#define l long long
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);
  int te; cin>>te;
  while(te--){
  l n; cin>>n;
   l ans ,a;
   cin>>ans;
   for(int i = 1; i < n;i++){
    cin>>a;
     ans = (ans&a);

   }

   cout<<ans<<"\n";
  }
}
