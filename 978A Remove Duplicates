#include<iostream>
using namespace std;
#include<vector>
#include<map>
int main(){
    int n ; cin>>n ;
    vector< int > vc(n);
    map< int , int >mp;
    for( int i =0 ; i< n ; i++){
        cin>>vc[i];
        mp[ vc[i] ] ++;


    }
    auto it = vc.begin();
    while ( it != vc.end()){
         if( mp[ *it ]> 1){
                      mp [ *it ]--;
                      it = vc.erase(it);
        }
        else it++;
    }
    cout<<vc.size()<<"\n";
    it = vc.begin();
    while( it != vc.end()){
        cout<< *it<<" ";
        it++;
    }

    }
