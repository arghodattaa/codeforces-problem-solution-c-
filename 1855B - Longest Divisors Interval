#include<iostream>
using namespace std;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);

   int te; cin>>te;
   while(te--){
      long long n; cin>>n;
     /* if( n == 1){
        cout<<1<<"\n";
        continue;
      }*/
      long long i = 1;
      while( n%i == 0) i++;
      /*for(  i =1; i<n; i++){
        if( n%i != 0) break;
      }*/
      cout<<i-1<<"\n";
   }
}
