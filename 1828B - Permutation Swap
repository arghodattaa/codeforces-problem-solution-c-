#include<iostream>
using namespace std;
#include<bits/stdc++.h>
int main(){
    int te; cin>>te;
    while( te --){
        int n ; cin>>n;
        int a;
        int ans = 0;
        for( int i  = 1; i<=n; i++){
            cin>>a;
            if(abs(a-i))
            ans =  __gcd(ans , abs(a-i));
        }
        cout<<ans<<"\n";
    }
}
