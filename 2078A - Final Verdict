#include<iostream>
using namespace std;
int main(){
  int te ; cin>>te ;
  while( te --){
    double n , x ;
    cin>>n >>x ;
    double sum = 0;
    int a ;
    for( int i = 0; i<n ; i++){
        cin>>a;
        sum += a;
    }
    sum /= n;
    if( sum ==x )cout<< "YES\n";
    else cout<<"NO\n";
  }
}
