#include<iostream>
using namespace std;
int main(){
 int te; cin>>te;
 while(te--){
    int x,y,a;
    cin>>x>>y>>a;
    int nn = a/(x+y);
    int ans = nn*(x+y);
    while(1){
            ans += x;
        if(ans > a){
                cout<<"NO\n";
        break;
        }
        ans += y;
        if( ans > a){
            cout<<"YES\n";
            break;
        }

    }
 }
}
