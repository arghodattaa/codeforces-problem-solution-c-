#include<iostream>
using namespace std;
#include<bits/stdc++.h>
int main(){
    long long int te;cin>>te;
    while(te--){
       long long  int a,b;
        cin>>a>>b;
        if(b==1){
            cout<<"NO"<<endl;
            continue;
        }cout<<"YES"<<endl;
        if(a==(a*(b-1))){
            cout<<a<<" "<<2*a*b-a<<" "<<2*a*b<<endl;
            continue;
        }

        cout<<a<<" "<<a*b<<" "<<a*(b+1)<<endl;
    }
}
